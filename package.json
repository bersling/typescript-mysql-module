{
  "name": "tsmysql",
  "version": "1.0.3",
  "description": "A database layer for a mysql database written in typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pub": "git add --all && git commit -m 'update' && git push && npm version patch && npm publish",
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "mocha --reporter spec --timeout 15000  --compilers ts:ts-node/register 'src/**/*.test.ts'",
    "windows_test": "mocha --reporter spec --timeout 15000 --compilers ts:ts-node/register 'src/**\\*.test.ts'",
    "spec": "mocha --reporter spec --compilers ts:ts-node/register --grep ${TEST} 'src/**/*.test.ts'",
    "lint": "tslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bersling/typescript-mysql-module.git"
  },
  "author": "Daniel Niederberger <bersling@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bersling/typescript-mysql-module/issues"
  },
  "homepage": "https://github.com/bersling/typescript-mysql-module#readme",
  "dependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "mocha": "^3.4.2",
    "mysql": "^2.13.0"
  },
  "devDependencies": {
    "@types/mysql": "^0.0.33",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "ts-node": "^3.0.6",
    "typescript": "^2.3.4"
  }
}
